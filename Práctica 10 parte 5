#include <stdio.h>

int main() {
    int matriz[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    int i = 0, cont = 0, *ap;
    ap = *matriz; // Equivale a: ap = &matriz[0][0];

    printf("Imprimir Matriz\n");

    do {
        if (cont == 3) {      // Se imprimió un renglón
            printf("\n");
            cont = 0;
        }

        printf("%d\t", *(ap + i)); // Imprime elemento usando apuntador
        cont++;
        i++;

    } while (i < 9);

    printf("\n");
    return 0;
}
