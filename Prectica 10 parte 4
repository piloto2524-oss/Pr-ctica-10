#include <stdio.h>

int main() {
    int matriz[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    int i, cont = 0, *ap;

    ap = *matriz; // Equivale a: ap = &matriz[0][0];

    printf("Imprimir Matriz\n");

    for (i = 0; i < 9; i++) {
        if (cont == 3) {     // Ya se imprimió un renglón
            printf("\n");
            cont = 0;
        }

        printf("%d\t", *(ap + i)); // Imprime el elemento i usando aritmética de apuntadores
        cont++;
    }

    printf("\n");
    return 0;
}
